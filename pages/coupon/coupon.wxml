<!--我的优惠券-->
<view class="coupon-container">
      <view class="filter-tab">
      <text class="'filter-text' {{item.name == curTabName ? 'active' : ''}}" bindtap='switchTab' data-name="{{item.name}}" wx:for="{{tabList}}"  wx:key="*this">{{item.text}}</text>
   </view>
</view>
<view class="coupon-list">
  <view class="coupon-item" wx:for="{{curListData}}" wx:key="*this">
     <view class="coupon-item-l">
        <view class="per-price">￥<text class="money">{{item.parPrice}}</text></view>
        <text class="price-desc">{{item.priceDesc}}</text>
     </view>
     <view class="coupon-item-r">
        <text class="coupon-desc">{{item.couponDesc}}</text>
        <text class="related-date">有效期至{{item.relatedDate}}</text>
     </view>
  </view>
</view>
